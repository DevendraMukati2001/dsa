#include<iostream>
using namespace std;
int main()
{
    int n,target,i;
    cin>>n;
    int arr[n];
    int flag=0;
    for(int i=0;i<n;i++)
    {
      cin>>arr[i];
    }
    cin>>target;
    for(int i=0;i<n;i++)
    {
        
        if(arr[i]==target){
        cout<<"mil gya bhai "<<target;
        flag=1;
        break;
        }
    }
    if (!flag) {
        cout << "Target not found in the array" << endl;
    }

    return 0;
}
