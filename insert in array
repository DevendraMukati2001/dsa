#include<iostream>
using namespace std;
int main(){
   int m[10],ins,i,n,pos;
   cout<<"how many element";
   cin>>n;
   for(int i=0;i<n;i++)
   {
       cin>>m[i];
   }
   cin>>ins;
   cout<<"position konsi :\n";
   cin>>pos;
   for(i=n;i>pos;i--)
   {
       m[i]=m[i-1];
   }
   m[pos]=ins;
   n++;
   for(int i=0;i<n;i++)
   {
       cout<<" "<<m[i];
   }
}
