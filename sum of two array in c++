#include<iostream>
using namespace std;

int main() {
   
    int arr1[5] = {1, 2, 3, 4, 5};
    int arr2[5] = {1, 2, 3, 4, 5};
    int n = 5; // Number of elements in the arrays

   

    // Print the sum array
    for(int i = 0; i < n; i++) {
        cout <<  (arr1[i] + arr2[i]) << " ";
    }

    return 0;
}
